<?xml version="1.0" encoding="utf-8"?>

<!-- These resources are around just to allow their values to be customized
     for different hardware and product builds. -->
<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- Enable USB mode selection -->
    <bool name="config_enableUsbModeSelection">true</bool>

    <!-- configurations to control what components of status bar are showed under keyguard -->
    <bool name="config_show_status_bar_battery_for_keyguard">true</bool>
    <bool name="config_show_status_bar_carrier_for_keyguard">true</bool>
    <bool name="config_show_status_bar_notification_for_keyguard">true</bool>
    <bool name="config_show_status_bar_signal_for_keyguard">true</bool>
    <bool name="config_show_status_bar_status_for_keyguard">true</bool>
    <bool name="config_show_status_bar_time_for_keyguard">false</bool>

    <!--  Has Antispam or not-->
    <bool name="config_has_antispam">true</bool>

    <!-- Default value of ANTI_STRANGER_CALL_ENABLED -->
    <integer name="anti_stranger_call_enabled">1</integer>

     <!-- Default LED freq time for notification LED in milliseconds. -->
    <integer name="config_defaultNotificationLedFreq">4000</integer>

    <!-- Default value of show rounded corners -->
    <integer name="config_show_rounded_corners_default">1</integer>

    <fraction name="config_full_screen_expand_status_bar_height_ratio">50%</fraction>

    <bool name="enable_magnifier_when_input">false</bool>

    <!-- on some devices, the first sensor change result may be invalid. so we have to delay for a certain time to wait next valid result -->
    <integer name="config_screen_on_proximity_sensor_first_change_delay">10</integer>

    <!-- config for mp3 record quality -->
    <integer name="config_mp3_record_quality">2</integer>

    <!-- config for dirac -->
    <bool name="is_dirac_supported">false</bool>

    <!-- config for dolby -->
    <bool name="is_dolby_supported">false</bool>

    <!-- default ringtone settings -->
    <string name="def_ringtone" translatable="false">/system/media/audio/ringtones/MI.ogg</string>
    <string name="def_alarm_alert" translatable="false">/system/media/audio/alarms/GoodMorning.ogg</string>
    <string name="def_notification_sound" translatable="false">/system/media/audio/notifications/FadeIn.ogg</string>
    <string name="def_sms_delivered_sound" translatable="false">/system/media/audio/ui/MessageComplete.ogg</string>
    <string name="def_sms_received_sound" translatable="false">/system/media/audio/notifications/FadeIn.ogg</string>

    <!-- config for voice wakeup -->
    <bool name="is_voice_wakeup_supported">false</bool>
</resources>
