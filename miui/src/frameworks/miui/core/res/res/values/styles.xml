<resources>
    <!-- Global Theme Styles -->
    <eat-comment />

    <!-- styles copied from android framework. It is a pity that we can't use alias
         mechanism to refer to android private style. Luckily, this is very rare and
         hopefully furture version will make these styles public
    -->
    <style name="TextViewFloatPanelButton">
        <item name="android:singleLine">true</item>
        <item name="android:textAppearance">?android:attr/textAppearanceSmall</item>
        <item name="android:shadowColor">#ccffffff</item>
        <item name="android:shadowRadius">0.2</item>
        <item name="android:shadowDy">1</item>
        <item name="android:textColor">@miui:color/textview_floatpanel_button_text_color</item>
        <item name="android:minWidth">60dip</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:minHeight">38dip</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@miui:drawable/textview_floatpanel_button_bg</item>
        <item name="android:textSize">@miui:dimen/v5_text_font_size_textview_float_panel_button</item>
    </style>

    <!-- default AlphabetFastIndexer style -->
    <style name="AlphabetFastIndexerStyle">
        <item name="miui:indexerTable">@array/alphabet_table</item>
        <item name="miui:indexerTextSize">@dimen/alphabet_indexer_text_size</item>
        <item name="miui:indexerTextColor">@color/alphabet_indexer_text_color</item>
        <item name="miui:indexerTextHighlightColor">@color/alphabet_indexer_highlight_text_color</item>
        <item name="miui:indexerBackground">@drawable/alphabet_indexer_bg</item>
        <item name="miui:overlayBackground">@drawable/alphabet_indexer_overlay</item>
        <item name="miui:overlayMarginLeft">@dimen/alphabet_indexer_overlay_offset</item>
        <item name="miui:overlayMarginTop">@dimen/alphabet_indexer_overlay_padding_top</item>
        <item name="miui:overlayTextSize">@dimen/alphabet_indexer_overlay_text_size</item>
        <item name="miui:overlayTextColor">@color/alphabet_indexer_overlay_text_color</item>
    </style>

    <!-- AlphabetFastIndexer style with '*' -->
    <style name="AlphabetFastIndexerStyleWithStarred" parent="AlphabetFastIndexerStyle">
        <item name="miui:indexerTable">@array/alphabet_table_with_starred</item>
    </style>

