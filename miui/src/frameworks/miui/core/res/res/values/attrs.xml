    <!-- =============================== -->
    <!--   MiuiProgressBar attributes    -->
    <!-- =============================== -->
    <declare-styleable name="MiuiProgressBar">
        <attr name="progressBar" format="reference"/>
        <attr name="progressBarMask" format="reference"/>
    </declare-styleable>

    <!-- =============================== -->
    <!--   MultiPositionBar attributes    -->
    <!-- =============================== -->
    <declare-styleable name="MultiPositionBar">
        <attr name="entries" format="reference"/>
        <attr name="entryValues" format="reference"/>
        <attr name="positionImage" format="reference"/>
    </declare-styleable>

    <!-- =============================== -->
    <!-- MIUI ROM APP data update -->
    <!-- =============================== -->
    <declare-styleable name="DataUpdate">
        <attr name="serviceName" format="string"/>
    </declare-styleable>
</resources>

