    <declare-styleable name="ActionMenuView">
        <attr name="v5_action_menu_view_more_icon" format="reference" />
        <attr name="v5_action_menu_view_secondary_container_layout" format="reference" />
        <attr name="v5_action_menu_view_expaned_background" format="reference" />
