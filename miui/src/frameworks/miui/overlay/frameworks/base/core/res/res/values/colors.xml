<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- 以下是MIUI新加的颜色定义，原生系统没有单独提取出这些颜色定义 -->
    <add-resource type="color" name="menu_text_color" />
    <add-resource type="color" name="tab_text_color" />
    <add-resource type="color" name="tab_pressed_text_color" />
    <add-resource type="color" name="edit_text_color" />
    <add-resource type="color" name="list_separator_foreground_holo_light" />
    <add-resource type="color" name="notification_action_text_color" />
    <add-resource type="color" name="statusbar_content" />
    <add-resource type="color" name="statusbar_content_title" />
    <add-resource type="color" name="statusbar_content_info" />
    <add-resource type="color" name="statusbar_content_time" />
    <add-resource type="color" name="statusbar_content_emphasis" />
    <add-resource type="color" name="notification_action_text_color" />

    <!-- 列表分割符文本颜色， 不要直接使用这个颜色，使用?android:attr/listSeparatorTextViewStyle -->
    <color name="list_separator_foreground_holo_light">#ffff7e00</color>

    <!-- 输入框选中内容的提示颜色 -->
    <color name="highlighted_text_holo_light">#ffffc960</color>
    <color name="statusbar_content">#999999</color>
    <color name="statusbar_content_title">#ffffff</color>
    <color name="statusbar_content_info">#999999</color>
    <color name="statusbar_content_time">#999999</color>
    <color name="statusbar_content_emphasis">#CCCCCC</color>
    <color name="notification_action_text_color">#ccc</color>

    <!-- 虚线分割  -->
    <drawable name="list_dashed_divider">@drawable/list_dashed_divider</drawable>
    <!-- 截图action背景  -->
    <drawable name="big_picture_action_bg">@drawable/big_picture_action_bg</drawable>
    <!-- 截图前景 处理圆角  -->
    <drawable name="big_picture_fg">@drawable/big_picture_fg</drawable>

    <drawable name="notification_bg">#00000000</drawable>
    <drawable name="notification_bg_low">#00000000</drawable>
</resources>
