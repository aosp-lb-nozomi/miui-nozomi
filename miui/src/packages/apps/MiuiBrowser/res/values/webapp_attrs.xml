<resources>
  <!-- @hide -->
  <declare-styleable name="FlingLayout" >
    <attr name="remove_src" format="reference" />
  </declare-styleable>
  <declare-styleable name="SplashView" >
    <attr name="splash_icon_top" format="dimension" />
    <attr name="splash_appname_relative_top" format="dimension" />
    <attr name="splash_copyright_bottom" format="dimension" />
    <attr name="loading_text_bottom" format="dimension" />
    <attr name="loading_relative_bottom" format="dimension" />
    <attr name="heart_animation" format="boolean" />
  </declare-styleable>
  <declare-styleable name="AsyncImageView" >
    <attr name="round_x" format="dimension" />
    <attr name="round_y" format="dimension" />
    <attr name="image_width" format="dimension" />
    <attr name="image_height" format="dimension" />
  </declare-styleable>
</resources>
