        <recharge-item
            key="MSGPAY"
            name="message"
            title="@string/recharge_message_title"
            icon="@drawable/ic_message"
            class="com.xiaomi.payment.ui.MessageActivity"
            visible="false" />
        <recharge-item
            key="VOUCHER"
            name="voucher"
            title="@string/recharge_voucher_title"
            icon="@drawable/ic_voucher"
