    <recharge-group
        key="others"
        title="@string/recharge_group_others">
        <recharge-item
            key="BANKCALLPAY"
            name="bankcall"
            title="@string/recharge_bankcall_title"
            icon="@drawable/ic_bankcall"
            class="com.xiaomi.payment.ui.BankCallActivity"
            visible="false" />
        <recharge-item
            key="CARDPAY"
            name="prepaid"
            title="@string/recharge_prepaid_title"
            icon="@drawable/ic_prepaid"
            class="com.xiaomi.payment.ui.PrepaidCardTypeActivity"
