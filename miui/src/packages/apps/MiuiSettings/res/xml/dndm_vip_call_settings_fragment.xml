<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen android:key="key_vip_call_setting_root"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- 开关 -->
    <PreferenceCategory>
        <CheckBoxPreference
            android:key="key_vip_call_button"
            android:title="@string/dndm_vip_call_button_title"
            android:persistent="false" >
        </CheckBoxPreference>
    </PreferenceCategory>

    <!-- VIP名单选项列表 -->
    <miui.preference.RadioButtonPreferenceCategory android:title="@string/dndm_vip_list_group"
        android:key="key_vip_list_group">
    </miui.preference.RadioButtonPreferenceCategory>

    <!-- 自定义名单 -->
    <PreferenceCategory android:key="key_vip_list_custom_category"
        android:title="@string/dndm_vip_list_custom_group" >
    </PreferenceCategory>
</PreferenceScreen>
