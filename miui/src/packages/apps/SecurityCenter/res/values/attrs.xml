<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="OptimizeItemView">
        <attr name="itemName" format="string" />
        <attr name="itemIcon" format="reference" />
        <attr name="tipsVisibility">
            <enum name="visible" value="0" />
            <enum name="invisible" value="4" />
            <enum name="gone" value="8" />
        </attr>
    </declare-styleable>
    <declare-styleable name="MenuView">
        <attr name="menuTitle" format="string" />
        <attr name="menuSummary" format="string" />
        <attr name="menuIcon" format="reference" />
        <attr name="fancyIcon" format="string" />
    </declare-styleable>

</resources>