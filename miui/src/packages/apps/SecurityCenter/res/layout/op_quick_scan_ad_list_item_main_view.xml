<?xml version="1.0" encoding="utf-8"?>
<com.miui.optimizecenter.view.QuickScanAdListItemMainView xmlns:android="http://schemas.android.com/apk/res/android"
    style="@style/ListItemMainView" >

    <ImageView
        android:id="@+id/arrow"
        style="@style/ListItemMainViewArrow" />

    <ImageView
        android:id="@+id/icon"
        style="@style/ListItemMainViewIcon"
        android:layout_toRightOf="@id/arrow" />

    <CheckBox
        android:id="@+id/checkbox"
        style="@style/ListItemMainViewCheckbox" />

    <TextView
        android:id="@+id/content"
        style="@style/ListItemMainViewContentText"
        android:layout_toLeftOf="@id/checkbox" />

    <TextView
        android:id="@+id/title"
        style="@style/ListItemMainViewTitleText"
        android:layout_toLeftOf="@id/content"
        android:layout_toRightOf="@id/icon" />

</com.miui.optimizecenter.view.QuickScanAdListItemMainView>