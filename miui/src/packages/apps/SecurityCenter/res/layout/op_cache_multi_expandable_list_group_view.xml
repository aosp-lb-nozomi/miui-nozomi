<?xml version="1.0" encoding="utf-8"?>
<com.miui.optimizecenter.view.CacheMultiExpandableListGroupView xmlns:android="http://schemas.android.com/apk/res/android"
    style="@style/ListItemMainView" >

    <ImageView
        android:id="@+id/icon"
        style="@style/ListItemMainViewIcon"
        android:layout_marginLeft="36dip" />

    <com.miui.optimizecenter.view.StateButton
        android:id="@+id/state_btn"
        style="@style/ListItemMainViewCheckbox" />

    <TextView
        android:id="@+id/content"
        style="@style/ListItemMainViewContentText"
        android:layout_toLeftOf="@id/state_btn" />

    <TextView
        android:id="@+id/title"
        style="@style/ListItemMainViewTitleText"
        android:layout_toLeftOf="@id/content"
        android:layout_toRightOf="@id/icon" />

</com.miui.optimizecenter.view.CacheMultiExpandableListGroupView>